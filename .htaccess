RewriteEngine On

# Handle static files first (CSS, JS, images)
RewriteCond %{REQUEST_FILENAME} -f
RewriteRule ^(.*\.(css|js|png|jpg|jpeg|gif|ico|svg))$ $1 [L]

# Handle clean URLs - redirect all other requests to index.php
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ index.php [QSA,L]

# Alternative: If the above doesn't work, try this simpler version
# RewriteRule ^(.*)$ index.php [QSA,L]